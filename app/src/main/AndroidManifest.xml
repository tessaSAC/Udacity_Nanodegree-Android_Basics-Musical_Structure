<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.android.musicalstructure">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name=".AllSongsActivity"
            android:label="All Songs"/>

        <activity
            android:name=".SingleAlbumActivity"
            android:label="Single Album" />

        <activity
            android:name=".AllAlbumsActivity"
            android:label="All Albums"/>

        <activity
            android:name=".PurchaseActivity"
            android:label="Make a Purchase"/>

        <activity
            android:name=".SingleArtistActivity"
            android:label="Single Artist" />
    </application>

</manifest>